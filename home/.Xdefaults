*VT100*translations: #override Shift <Btn1Up>: exec-formatted("firefox '%t'", PRIMARY)

*saveLines: 1000000
*scrollBar: false
*font: xft:Inconsolata:autohint=true:antialias=true:pixelsize=13
*borderless: true

*scrollTtyOutput: false
*scrollWithBuffer: true
*scrollTtyKeypress: true

URxvt.iso14755: false
URxvt.perl-ext: clipboard,matcher,default
URxvt*urlLauncher: /usr/bin/google-chrome
URxvt*matcher.button: 1
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~
/* URxvt.termName: rxvt-256color */
URxvt.title: terminal
